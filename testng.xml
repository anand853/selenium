					<include name="clickOnRoomba900" />


					<!-- 3rd page -->
					<include name="verifyThirdPageTitle" />
					<include name="verifyDetectedRobots" />
					<include name="verifySetUpNewRoomba" />
					<include name="verifyLearnMore" />

					<!-- 4th page -->
					<include name="verifyFourthPageTitle" />
					<include name="verifyHelloText" />
					<include name="verifyHelloDesc" />
					<include name="verifyDots" />
					<include name="verifySkip" />
