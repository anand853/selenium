<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Altadena Automation Report version (1.6)" verbose="1">


	<test name="Aspen Mobile Test" preserve-order="true">
		<classes>
			<class name="com.google.test.Roomba900">
				<methods>

					<include name="installAPK" />
					<include name="verifyAcceptAgreementText" />
					<include name="verifyAcceptAgreementCheckBox" />
					<include name="verifyAcceptAgreementReview" />

					<include name="AppLangText" />
					<include name="Automatic" />
					<include name="stayInformed" />
					<include name="aboutThisApp" />

					<include name="verifyButtonNextDisabled" />
